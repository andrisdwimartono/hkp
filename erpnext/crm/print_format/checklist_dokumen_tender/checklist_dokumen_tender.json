 "html": "<table width=\"100%\">\n    <tr>\n        <td width=\"65%\">\n            {{ frappe.db.get_value(\"Letter Head\", {\"name\": \"KOP HKP\"}, [\"content\"]) }}\n        </td>\n        <td width=\"35%\" style=\"padding-right: 2px !important;\">\n            <table style=\"font-size: 10px; border:1px solid black; margin: 0px; padding: 0px;\" class=\"ml-auto\">\n                <tr>\n                    <td style=\"padding: 2px !important;\">Dokumen</td>\n                    <td style=\"padding: 2px !important;\">: HKP-FI-SAR-11</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Revisi</td>\n                    <td style=\"padding: 2px !important;\">: 01</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Tgl.Terbit</td>\n                    <td style=\"padding: 2px !important;\">: 25 Juli 2014</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n</table>\n\n<div class=\"text-center\">\n    <h4>CHECKLIST DOKUMEN TENDER</h4>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td style=\"width:25%\">Nama Paket</td>\n                    <td style=\"width:2%\">:</td>\n                    <td style=\"width:73%\">{{doc.nama_paket or \"\"}}</td>\n                </tr>\n                \n                <tr>\n                    <td>No. RKS</td>\n                    <td>:</td>\n                    <td>{{doc.no_rks or \"\"}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Tanggal</td>\n                    <td>:</td>\n                    <td>{{doc.get_formatted(\"tanggal\")}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Tanggal Pemasukan</td>\n                    <td>:</td>\n                    <td>{{doc.get_formatted(\"tanggal_pemasukan\")}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Tanggal Pengecekan</td>\n                    <td>:</td>\n                    <td>{{doc.get_formatted(\"tanggal_pengecekan\")}}</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Checklist -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial; collapse: collapse\">\n            <tbody>\n                <tr>\n                    <td class=\"border-table\" style=\"font-weight: bold; text-align: center; width: 5%\">NO</td>\n                    <td class=\"border-table\" style=\"font-weight: bold; text-align: center; width: 30%\">URAIAN</td>\n                    <td class=\"border-table\" style=\"font-weight: bold; text-align: center; width: 10%\">ADA</td>\n                    <td class=\"border-table\" style=\"font-weight: bold; text-align: center; width: 10%\">TIDAK ADA</td>\n                    <td class=\"border-table\" style=\"font-weight: bold; text-align: center; width: 35%\">KETERANGAN</td>\n                </tr>\n                \n                {%- for datum in doc.checklist -%}\n                <tr>\n                    <td class=\"border-table\">{{ 1 }}</td>\n                    <td class=\"border-table\">{{ datum.uraian }}</td>\n                    <td class=\"border-table text-center\">{% if datum.ada %}\u2713{% endif %}</td>\n                    <td class=\"border-table text-center\">{% if datum.tidak_ada %}\u2713{% endif %}</td>\n                    <td class=\"border-table\">{{ datum.keterangan or \"\" }}</td>\n                </tr>\n                {%- endfor -%}\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Saran-saran -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Surabaya, ...........</td>\n                </tr>\n                \n                <tr style=\"height: 100px\">\n                    <td style=\"vertical-align: bottom\">Manager Pemasaran</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>",
