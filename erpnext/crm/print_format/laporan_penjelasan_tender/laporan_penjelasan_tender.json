 "html": "<table width=\"100%\">\n    <tr>\n        <td width=\"65%\">\n            {{ frappe.db.get_value(\"Letter Head\", {\"name\": \"KOP HKP\"}, [\"content\"]) }}\n        </td>\n        <td width=\"35%\" style=\"padding-right: 2px !important;\">\n            <table style=\"font-size: 5px; border:1px solid black; margin: 0px; padding: 0px;\" class=\"ml-auto\">\n                <tr>\n                    <td style=\"padding: 2px !important;\">Dokumen</td>\n                    <td style=\"padding: 2px !important;\">: HKP-FI-DKD-01</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Revisi</td>\n                    <td style=\"padding: 2px !important;\">: 04</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Tgl.Terbit</td>\n                    <td style=\"padding: 2px !important;\">: 25 Juli 2014</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n</table>\n\n<div class=\"text-center\">\n    <h4>LAPORAN PENJELASAN TENDER</h4>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Tanggal</td>\n                    <td>:</td>\n                    <td>{{doc.tanggal or \"\"}}</td>\n                    \n                    <td>Atas Nama</td>\n                    <td>:</td>\n                    <td>{{doc.atas_nama or \"\"}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Pelanggan</td>\n                    <td>:</td>\n                    <td>{{doc.pelanggan or \"\"}}</td>\n                    \n                    <td>RKS</td>\n                    <td>:</td>\n                    <td>{{doc.rks or \"\"}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Lingkup Pekerjaan</td>\n                    <td>:</td>\n                    <td>{{doc.lingkup_pekerjaan or \"\"}}</td>\n                    \n                    <td>Penjelasan</td>\n                    <td>:</td>\n                    <td>\n                        <span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Kantor' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Kantor\n                        <br>\n                        <span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Lapangan' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Lapangan\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<div class=\"text-center\">\n    <h4>URAIAN SINGKAT</h4>\n</div>\n\n<!-- Penjelasan Administrasi -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Penjelasan Administrasi</td>\n                </tr>\n                \n                <tr style=\"height: 100px\">\n                    <td>{{doc.penjelasan_administrasi or \"\"}}</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Penjelasan Teknis -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Penjelasan Teknis</td>\n                </tr>\n                \n                <tr style=\"height: 100px\">\n                    <td>{{doc.penjelasan_teknis or \"\"}}</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Pengambilan Berita Acara -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Pengambilan Berita Acara</td>\n                    <td>Tanggal</td>\n                    <td>:</td>\n                    <td>{{doc.tanggal_pengambilan_berita_acara or \"\"}}</td>\n                    \n                    <td>Jam</td>\n                    <td>:</td>\n                    <td>{{doc.jam_pengambilan_berita_acara or \"\"}}</td>\n                </tr>\n                \n                <tr>\n                    <td>Pemasukan Penawaran</td>\n                    <td>Tanggal</td>\n                    <td>:</td>\n                    <td>{{doc.tanggal_pemasukan_penawaran or \"\"}}</td>\n                    \n                    <td>Jam</td>\n                    <td>:</td>\n                    <td>{{doc.jam_pemasukan_penawaran or \"\"}}</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Peserta Tender -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Peserta Tender</td>\n                </tr>\n                \n                <tr>\n                    <td></td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Saran-saran -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Saran-saran</td>\n                    <td>Surabaya, ...........</td>\n                </tr>\n                \n                <tr style=\"height: 100px\">\n                    <td>{{doc.saran_saran