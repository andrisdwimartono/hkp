 "html": "<table width=\"100%\">\n    <tr>\n        <td width=\"65%\">\n            {{ frappe.db.get_value(\"Letter Head\", {\"name\": \"KOP HKP\"}, [\"content\"]) }}\n        </td>\n        <td width=\"35%\" style=\"padding-right: 2px !important;\">\n            <table style=\"font-size: 5px; border:1px solid black; margin: 0px; padding: 0px;\" class=\"ml-auto\">\n                <tr>\n                    <td style=\"padding: 2px !important;\">Dokumen</td>\n                    <td style=\"padding: 2px !important;\">: HKP-FI-SAR-06</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Revisi</td>\n                    <td style=\"padding: 2px !important;\">: 03</td>\n                </tr>\n                \n                <tr>\n                    <td style=\"padding: 2px !important;\">Tgl.Terbit</td>\n                    <td style=\"padding: 2px !important;\">: 25 Juli 2014</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n</table>\n\n<div class=\"text-center\">\n    <h4>INFORMASI PEMBUKAAN TENDER</h4>\n</div>\n\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Lingkup Pekerjaan</td>\n                    <td>:</td>\n                    <td>{{doc.lingkup_pekerjaan or \"\"}}</td>\n                    \n                    <td>Untuk</td>\n                    <td>:</td>\n                    <td>\n                        <span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Kantor' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Direktur Operasi\n                    </td>\n                    <td>\n                        <span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Kantor' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Estimasi\n                    </td>\n                </tr>\n                \n                <tr>\n                    <td>Pelanggan</td>\n                    <td>:</td>\n                    <td>{{doc.pelanggan or \"\"}}</td>\n                    \n                    <td></td>\n                    <td></td>\n                    <td><span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Kantor' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Sekretariat\n                    </td>\n                </tr>\n                \n                <tr>\n                    <td></td>\n                    <td></td>\n                    <td></td>\n                    \n                    <td>Tembusan</td>\n                    <td>:</td>\n                    <td>\n                        <span style=\"font-size: 10pt\">\n                        {% if doc.penjelasan != 'Kantor' %}\n                            &#x2610;\n                        {% else %}\n                            &#x2611;\n                        {% endif %}\n                        </span>\n                        &nbsp; Direktur Utama\n                    </td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Kami informasikan... -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td>Kami informasikan hasil pembukaan tender sebagai berikut :</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Peserta Tender -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr style=\"border: 1px solid black\">\n                    <th class=\"text-center\" style=\"border: 1px solid black\">No.</th>\n                    <th class=\"text-center\" style=\"border: 1px solid black\">Nama Peserta</th>\n                    <th class=\"text-center\" style=\"border: 1px solid black\">Harga Penawaran <br> (Rp. 000)</th>\n                    <th class=\"text-center\" style=\"border: 1px solid black\">Keterangan</th>\n                </tr>\n                \n                {%- for datum in doc.hasil_pembukaan_tender -%}\n                <tr style=\"border: 1px solid black\">\n                    <td style=\"border: 1px solid black\"></td>\n                    <td style=\"border: 1px solid black\"></td>\n                    <td style=\"border: 1px solid black\"></td>\n                    <td style=\"border: 1px solid black\"></td>\n                </tr>\n                {%- endfor -%}\n            </tbody>\n        </table>\n    </div>\n</div>\n\n<!-- Catatan -->\n<div class=\"row\">\n    <div class=\"col-12\">\n        <table style=\"font-size: 10pt; width: 100%; font-family: Arial\">\n            <tbody>\n                <tr>\n                    <td style=\"width: 75%\">Catatan:</td>\n                    <td style=\"width: 35%\">Surabaya, ...........</td>\n                </tr>\n                \n                <tr style=\"height: 100px\">\n                    <td>{{doc.catatan or \"\"}}</td>\n                    <td style=\"vertical-align: bottom\">(.....................)</td>\n                </tr>\n            </tbody>\n        </table>\n    </div>\n</div>",
