 "html": "<table width=\"100%\">\n    <tr>\n        <td width=\"65%\">\n            {{ frappe.db.get_value(\"Letter Head\", {\"name\": \"KOP HKP\"}, [\"content\"]) }}\n        </td>\n        <!--<td width=\"35%\" style=\"padding-right: 2px !important;\">-->\n        <!--    <table style=\"font-size: 10px; border:1px solid black; margin: 0px; padding: 0px;\" class=\"ml-auto\">-->\n        <!--        <tr>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">Dokumen</td>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">: HKP-FI-SAR-02</td>-->\n        <!--        </tr>-->\n        <!--        <tr>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">Revisi</td>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">: 05</td>-->\n        <!--        </tr>-->\n        <!--        <tr>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">Tgl.Terbit</td>-->\n        <!--            <td style=\"padding: 2px !important;\" class=\"text-left\">: 23 Maret 2023</td>-->\n        <!--        </tr>-->\n        <!--    </table>-->\n        <!--</td>-->\n    </tr>\n</table>\n<table width=\"100%\">\n    <thead>\n        <tr>\n            <td colspan=\"6\" style=\"font-size: 18px; padding: 5px !important\" class=\"text-center\"><b>MEMO</b></td>\n        </tr>\n    </thead>\n    <tbody style=\"font-size: 12px; padding: 5px !important\" class=\"text-left\">\n        <tr>\n            <td width=\"17%\">\n                Tanggal Surat\n            </td>\n            <td width=\"3%\">:</td>\n            <td width=\"30%\">{{doc.get_formatted(\"tanggal_memo\") or \"\"}}</td>\n            <td width=\"12%\" rowspan=3>\n                Kepada\n            </td>\n            <td width=\"3%\" rowspan=3>:</td>\n            <td width=\"35%\" rowspan=3>\n                {% for d in doc.kepada %}\n                    {% if d.type == 'Employee' %}\n                        {{ loop.index }}. {{ d.employee_name }}<br>\n                    {% else %}\n                        {{ loop.index }}. {{ d.pihak }}<br>\n                    {% endif %}\n                {% endfor %}\n            </td>\n        </tr>\n        <tr>\n            {% if doc.dari_merupakan_karyawan == 1 %}\n            <td>Dari</td>\n            <td>:</td>\n            <td>{{doc.dari_emoloyee_name or \"\"}}</td>\n            {% else %}\n            <td>Dari</td>\n            <td>:</td>\n            <td>{{doc.dari_designation or \"\"}}</td>\n            {% endif %}\n        </tr>\n        <tr>\n            <td>Tembusan</td>\n            <td>:</td>\n            <td>\n                {% for d in doc.tembusan %}\n                    {% if d.type == 'Employee' %}\n                        {{ loop.index }}. {{ d.employee_name }}<br>\n                    {% else %}\n                        {{ loop.index }}. {{ d.pihak }}<br>\n                    {% endif %}\n                {% endfor %}\n            </td>\n        </tr>\n        <tr>\n            <td>Hal</td>\n            <td>:</td>\n            <td colspan=4>{{doc.hal or \"\"}}</td>\n        </tr>\n        <tr>\n            <td colspan=6 style=\"padding: 15px 0px 0px 0px !important;\">\n                <table width=\"100%\" style=\"font-size: 12px; margin: 0px !important;\">\n                    <tr>\n                        <td>{{doc.pesan_1 or \"\"}}\n                        <table border=\"0\" style=\"font-size: 12px; padding: 5px !important; margin-left: 50px !important; margin-top: 8px !important;\">\n                            {% if doc.tanggal %}\n                            <tr>\n                                <td>Tanggal</td>\n                                <td>: {{doc.get_formatted(\"tanggal\") or \"-\"}}</td>\n                            </tr>\n                            {% endif %}\n                            {% if doc.dengan_waktu == 1 %}\n                            <tr>\n                                <td>Jam</td>\n                                <td>: {{doc.waktu_mulai or \"-\"}} s/d {{doc.waktu_selesai or \"-\"}}</td>\n                            </tr>\n                            {% endif %}\n                            {% if doc.tempat %}\n                            <tr>\n                                <td>Tempat</td>\n                                <td>: {{doc.tempat or \"-\"}}</td>\n                            </tr>\n                            {% endif %}\n                            {% if doc.agenda %}\n                            <tr>\n                                <td>Agenda</td>\n                                <td>: {{doc.agenda or \"-\"}}</td>\n                            </tr>\n                            {% endif %}\n                            {% if doc.keterangan %}\n                            <tr>\n                                <td>Keterangan</td>\n                                <td>: {{doc.keterangan or \"-\"}}</td>\n                            </tr>\n                            {% endif %}\n                            \n                        </table>\n                        </td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n        <tr>\n            <td colspan=6 style=\"padding: 15px 0px 0px 0px !important;\">\n                <table width=\"100%\" style=\"font-size: 12px; margin: 0px !important;\">\n                    <tr>\n                        <td>{{doc.pesan_2 or \"\"}}</td>\n                    </tr>\n                </table>\n            </td>\n        </tr>\n    </tbody>\n</table>",
