 "doctype": "Print Format",
 "font": "Default",
 "html": "<table width=\"100%\">\n    <tr>\n        <td width=\"65%\">\n            {{ frappe.db.get_value(\"Letter Head\", {\"name\": \"KOP HKP\"}, [\"content\"]) }}\n        </td>\n        <td width=\"35%\" style=\"padding-right: 2px !important;\">\n            <table style=\"font-size: 10px; border:1px solid black; margin: 0px; padding: 0px;\" class=\"ml-auto\">\n                <tr>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">Dokumen</td>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">: HKP-FI-SAR-02</td>\n                </tr>\n                <tr>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">Revisi</td>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">: 05</td>\n                </tr>\n                <tr>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">Tgl.Terbit</td>\n                    <td style=\"padding: 2px !important;\" class=\"text-left\">: 23 Maret 2023</td>\n                </tr>\n            </table>\n        </td>\n    </tr>\n</table>\n<table width=\"100%\">\n    <thead>\n        <tr>\n            <td colspan=\"3\" style=\"font-size: 18px; padding: 5px !important\" class=\"text-center\"><b>LAPORAN AKTIVITAS DI LUAR KANTOR</b></td>\n        </tr>\n    </thead>\n    <tbody style=\"font-size: 12px; padding: 5px !important\" class=\"text-left\">\n        <tr>\n            <td>\n                Jenis Aktivitas\n            </td>\n            <td colspan=\"2\">: {% if doc.jenis_aktivitas != \"Lainnya\" %} {{doc.jenis_aktivitas}} {% else %} {{doc.jenis_aktivitas_lainnya}} {% endif %}</td>\n        </tr>\n        <tr>\n            <td width=\"23%\">Tanggal</td>\n            <td width=\"50%\">: {{doc.get_formatted(\"tanggal\")}}</td>\n            <td width=\"27%\">Jam : {{doc.jam_mulai or \"-\"}} s/d {{doc.jam_selesai or \"-\"}}</td>\n        </tr>\n        <tr>\n            <td>Nama Pelanggan</td>\n            <td colspan=\"2\">: {{doc.customer or \"\"}}</td>\n        </tr>\n        <tr>\n            <td>Alamat</td>\n            <td colspan=\"2\">: {{doc.alamat or \"\"}}</td>\n        </tr>\n        <tr>\n            <td>Nama yang dikunjungi</td>\n            <td colspan=\"2\">\n                {% if doc.persons|length > 0 %}\n                <table width=\"100%\" style=\"font-size: 12px; padding: 5px !important\" class=\"text-left\" border=\"1\">\n                    <thead>\n                        <tr class=\"text-center\">\n                            <td width=\"60%\"><b>Nama</b></td>\n                            <td width=\"40%\"><b>Jabatan</b></td>\n                        </tr>\n                    </thead>\n                    <tbody>\n                 {% for d in doc.persons %}\n                        <tr>\n                            <td>{{d.nama or \"\"}}</td>\n                            <td>{{d.jabatan or \"\"}}</td>\n                        </tr>\n                 {% endfor %}\n                    </tbody>\n                </table>\n                {% endif %}\n            </td>\n        </tr>\n        <tr>\n            <td colspan=\"3\" class=\"text-center\" style=\"font-size: 14px; padding: 5px !important\">\n                <b>URAIAN AKTIVITAS</b>\n            </td>\n        </tr>\n        <tr>\n            <td colspan=\"3\">\n                {{doc.uraian_aktivitas or \"-\"}}\n            </td>\n        </tr>\n        <tr>\n            <td colspan=\"3\">&nbsp;</td>\n        </tr>\n        <!--<tr>-->\n        <!--    <td colspan=\"3\" class=\"text-center\" style=\"font-size: 14px; padding: 5px !important\">-->\n        <!--        <b>URAIAN PROSPEK</b>-->\n        <!--    </td>-->\n        <!--</tr>-->\n        <!--<tr>-->\n        <!--    <td colspan=\"3\">-->\n        <!--        {{doc.uraian_prospek or \"-\"}}-->\n        <!--    </td>-->\n        <!--</tr>-->\n        <!--<tr>-->\n        <!--    <td colspan=\"3\">&nbsp;</td>-->\n        <!--</tr>-->\n        <tr>\n            <td colspan=\"2\">\n                Disposisi:<br>\n                {{doc.disposisi or \"-\"}}\n            </td>\n            <td class=\"text-center\">\n                Yang Membuat Laporan<br><br><br><br><br><br>\n                {% set employees = frappe.get_all(\"Employee\", filters={\"user_id\": doc.owner}, fields=[\"first_name\"], order_by='name asc') %}\n                {% for employee in employees %}\n                {{employee.first_name}}\n                {% endfor %}\n            </td>\n        </tr>\n    </tbody>\n    \n</table>\n",
